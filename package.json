{
  "name": "gitlogmcp",
  "version": "1.1.2",
  "description": "MCP server for Git repository analysis with AI-powered code change analysis using OpenRouter",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "gitlogmcp": "build/index.js"
  },
  "files": [
    "build/**/*",
    "README.md",
    "SUMMARY.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "tsc --watch",
    "test": "jest --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "mcp",
    "git",
    "ai",
    "openrouter",
    "analysis",
    "code-analysis",
    "diff-analysis",
    "commit-analysis",
    "model-context-protocol",
    "claude",
    "gpt",
    "typescript"
  ],
  "author": "GitLogMCP Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gitlogmcp/gitlogmcp.git"
  },
  "homepage": "https://github.com/gitlogmcp/gitlogmcp#readme",
  "bugs": {
    "url": "https://github.com/gitlogmcp/gitlogmcp/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0",
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
    "simple-git": "^3.20.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
